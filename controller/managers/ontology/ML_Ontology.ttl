    rdfs:comment "SPARQL query to get AutoML solutions and the ML libraries they are used for" .
:query_get_all_predefined_sparql_queries a sp:Select ;
    skos:prefLabel "Query get all predefined SPARQL queries" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

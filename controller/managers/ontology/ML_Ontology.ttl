# spInsertData Inferencing

:inference_rule_broader_inheritance a sp:InsertData ;
    skos:prefLabel "Inference rule broader inheritance" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
INSERT {?s ?p ?o .}
WHERE  {?p a rdf:Property ;
                        :inheritance :broader_inherited .
        ?s skos:broader+ ?b .
        ?b ?p ?o .}''' ;
    rdfs:comment "SPARQL update statement to insert new triples with subjects that are in a (transitive) skos:broader relationship for existing triples with a predicate of inheritance type :broader_inherited" .


# spSelect Quality Checks

:check_class_instance a sp:Select ;
    skos:prefLabel "Check class instance" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT ?class ?subject ?predicate ?object
WHERE {
        ?subject ?predicate ?object .
        OPTIONAL {?subject a ?class .}
        FILTER isIRI(?object)
        FILTER(STRSTARTS(str(?object), str(:)))
        FILTER NOT EXISTS {?object a ?c}
}''' ;
    rdfs:comment "All objects should be instances of some class -> ok if query returns nothing; otherwise check ?object of triple in ?class" .
:check_unique_class a sp:Select ;
    skos:prefLabel "Check unique class" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT DISTINCT ?subject ?class1 ?class2
WHERE {
        ?subject a ?class1 .
        ?subject a ?class2 .
        FILTER(?class1 != ?class2)
}''' ;
    rdfs:comment "No object should be instance of several classes -> ok if query retuns nothing; otherwise check redundant ?class1 and ?class2" .
:check_predicate_declared a sp:Select ;
    skos:prefLabel "Check predicate declared" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT DISTINCT ?predicate
WHERE {
        ?s ?predicate ?o .
        FILTER(STRSTARTS(str(?predicate), str(:)))
        FILTER NOT EXISTS {?predicate a rdf:Property}
}''' ;
    rdfs:comment "Predicates should be declared -> ok if query returns nothing; otherwise check ?predicate" .
:check_predicates_with_domain_and_range_datatypes a sp:Select ;
    skos:prefLabel "Check predicates with domain and range datatypes" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
SELECT DISTINCT ?predicate ?domain ?range
WHERE {
        ?s ?predicate ?o .
        ?s a ?domain .
        ?o a ?range .
}
ORDER BY ?predicate''' ;
    rdfs:comment "Lists all used predicates and their subject / object datatypes -> check whether subject datatypes (?domain) and object datatypes (?range) make sense" .

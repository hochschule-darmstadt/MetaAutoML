# spSelect Queries

:query_get_all_dataset_types a sp:Select ;
    skos:prefLabel "Query get all dataset types" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT ?t
WHERE {
        ?t a :Enum;
           :category :dataset_type .
}''' ;
    rdfs:comment "SPARQL query to get all dataset types" .
:query_get_all_ml_tasks a sp:Select ;
    skos:prefLabel "Query get all ML tasks" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT ?t ?a
WHERE {
        ?t a :ML_task;
           :belongs_to ?a .
}''' ;
    rdfs:comment "SPARQL query to get all ML tasks an the ML areas they belong to" .
:query_get_all_ml_approaches a sp:Select ;
    skos:prefLabel "Query get all ML approaches" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT *
WHERE {
        ?a a :ML_approach;
           :used_for ?t .
}''' ;
    rdfs:comment "SPARQL query to get all ML approaches and the ML tasks they can be used for" .
:query_get_all_preprocessing_approaches a sp:Select ;
    skos:prefLabel "Query get all preprocessing approaches" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT *
WHERE {
        ?p a :Preprocessing_approach.
                  OPTIONAL {?p :used_for ?t} .
}''' ;
    rdfs:comment "SPARQL query to get all preprocessing approaches and the ML tasks they can be used for" .
:query_get_all_metrics a sp:Select ;
    skos:prefLabel "Query get all metrics" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT *
WHERE {
        ?m a :Metric;
           :used_for ?t .
}''' ;
    rdfs:comment "SPARQL query to get all performance metrics the the ML tasks they can be used for" .
:query_get_all_ml_libraries a sp:Select ;
    skos:prefLabel "Query get all ML libraries" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
SELECT *
WHERE {
        ?m a :ML_library ;
           skos:prefLabel ?l.
}''' ;
    rdfs:comment "SPARQL query to get all ML libraries with their names" .
:query_get_all_automl_solutions a sp:Select ;
    skos:prefLabel "Query get all AutoML solutions" ;
    sp:text '''PREFIX : <http://h-da.de/ml-ontology/>
SELECT *
WHERE {
        ?a a :AutoML_solution ;
           :used_for ?l.

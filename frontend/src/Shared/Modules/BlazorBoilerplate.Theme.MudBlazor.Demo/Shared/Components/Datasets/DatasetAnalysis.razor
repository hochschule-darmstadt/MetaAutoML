@inject IStringLocalizer<Global> L
@inject IApiClient apiClient
@inject IViewNotifier viewNotifier

<MudCard Elevation="5">
    <MudCardHeader Style="border-bottom-style: inset; border-bottom-width: 1px;">
        <CardHeaderContent >
            <MudText Typo="Typo.h6">Analysis</MudText>
        </CardHeaderContent>
        <CardHeaderActions>
            <MudButton Href="/analysis"
                Variant="Variant.Filled" 
                EndIcon="@Icons.Material.Filled.MoreHoriz" 
                Color="Color.Secondary"
                Size="Size.Small"
                 Style="margin-top: 8px">More</MudButton>
        </CardHeaderActions>
    </MudCardHeader>
    <MudCardContent>
        <MudPaper Height="250px" Elevation="0">
            @if (Dataset == null)
            {
                 <DataLoaderSpinner />
            }
            else
            {
            }
        </MudPaper>
    </MudCardContent>
</MudCard>

@code {
    [Parameter]
    public GetDatasetResponseDto Dataset { get; set; }
}

@inject IStringLocalizer<Global> L
@inject NavigationManager NavManager


@if (Status != null)
{
    <MudMenu Icon="@Icons.Material.Filled.MoreVert">
        <MudMenuItem OnClick="@(e => { NavManager.NavigateTo("trainings/" + TrainingId + "/models/" + Status.ID); })">Details</MudMenuItem>
        <MudMenuItem>Delete</MudMenuItem>
    </MudMenu>
}

@code {
    [Parameter]
    public string TrainingId { get; set; }
    [Parameter]
    public AutoMLStatusDto Status { get; set; }

}
